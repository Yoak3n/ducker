{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ducker",
  "version": "0.1.6",
  "identifier": "com.Yoaken.ducker",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [],    
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "nsis",
    "windows": {
        "nsis": {
          "displayLanguageSelector": true,
          "languages": ["SimpChinese","English"]
        }
      },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
